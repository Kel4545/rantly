<div class="user">
  <br>

  <div class="style"><%= link_to current_user.username, edit_user_path(@users), :class => "userlink" %></div>
  <%= image_tag current_user.url %>
  <ul>
    <%= render 'form' %>
  </ul>
</div>
<div class=rant_dashboard>
  <div class="position"><h1>Latest Rants</h1>
    <% other_users.each do |user| %>
      <%= user.username %>
      <%= link_to "Follow", user_path(@users), :class => "link" %>
    <% end %>
    <% @rant.each do |rant| %>
      <li><%= link_to rant.rant, user_rant_path(id: @rant), :class => "link" %></li> <%= link_to "Delete", user_rant_path(rant.user_id, rant.id), :class => "delete", :method => :delete %>
    <% end %>
    <h1>My Rants</h1>
    <% current_user.rants.each do |rant| %>
      <li><%= rant.about %></li>
      <li><%= link_to rant.rant, user_rant_path(id: @rant), :class => "link" %></li> <%= link_to "Delete", user_rant_path(rant.user_id, rant.id), :class => "delete", :method => :delete %>
    <% end %>
  </div>
</div>
